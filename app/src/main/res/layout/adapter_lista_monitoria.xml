<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"

    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/tv_AdapterMonitoria_Sala"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvQuaquer"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="7dp"
        android:layout_marginTop="8dp"
        android:text="Sala: 114"
        android:textAlignment="center"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/tvQuaquer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="18dp"
        android:layout_marginRight="3dp"
        android:text="Fim:"
        android:textSize="18sp"
        android:layout_alignBaseline="@+id/tv_adapterMonitoria_inici"
        android:layout_alignBottom="@+id/tv_adapterMonitoria_inici"
        android:layout_toStartOf="@+id/tv_AdapterMonitoria_fim"
        android:layout_marginEnd="12dp" />

    <TextView
        android:id="@+id/tv_adapterMonitoria_inici"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView15"
        android:layout_alignBottom="@+id/textView15"
        android:layout_marginLeft="3dp"
        android:layout_marginRight="8dp"
        android:layout_toEndOf="@+id/textView15"
        android:text="17:30"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/tv_AdapterMonitoria_DiaSemana"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/tv_AdapterMonitoria_Sala"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:text="Segunda"
        android:textAlignment="center"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/tv_AdapterMonitoria_DiaSemana"
        android:layout_below="@+id/tv_AdapterMonitoria_DiaSemana"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="3dp"
        android:layout_marginTop="8dp"
        android:text="Inicio:"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/tv_AdapterMonitoria_fim"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="3dp"
        android:layout_marginRight="16dp"
        android:text="18:30"
        android:textSize="18sp"
        android:layout_above="@+id/tv_AdapterMonitoria_Sala"
        android:layout_alignEnd="@+id/tv_AdapterMonitoria_DiaSemana"
        android:layout_marginEnd="40dp" />
</RelativeLayout>